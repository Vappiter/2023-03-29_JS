let a = -500;
let b = 15;

 if (Number(a) == NaN || Number(b) == NaN)
     {
     console.log('NaN');
     } 
 let i1 = a;
 let i2 = b;    
    while (i1 != 0 && i2 != 0)
    {
    if (i1 > i2)
     {
     i1 = i1 % i2;
     }   
    else
        {
        i2 = i2 % i1;
        }
        }
console.log (i1 + i2);
